{
    "name": "webfiori/jsonx",
    "type": "library",
    "description": "PHP library for creating well-formatted JSON strings.",
    "version":"3.3.0",
    "keywords": [
        "JSON", "PHP", "JSONx"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0"
    },
    "autoload" :{
        "psr-4":{
            "WebFiori\\Json\\":"WebFiori/Json"
        }
    },
    "autoload-dev" :{
        "psr-4":{
            "WebFiori\\Tests\\":"tests/WebFiori/Tests"
        }
    },
    "scripts":{
        "test": "phpunit --configuration tests/phpunit.xml"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
}
